{
  "name": "smart-sidebar",
  "version": "0.1.1",
  "homepage": "https://github.com/yaplas/smart-sidebar",
  "description": "jQuery plugin used to convert an element in a sidebar that follow the scroll in a smart way, even when sidebar is higher than viewport",
  "repository": {
    "type": "git",
    "url": "git://github.com/yaplas/smart-sidebar.git"
  },
  "main": "jquery-smart-sidebar.js",
  "dependencies": {
    "jquery": ">=1.8"
  },
  "keywords": [
    "jQuery",
    "scroll",
    "fixed",
    "sidebar",
    "affix",
    "higher",
    "viewport",
    "large",
    "sidebar",
    "vertical",
    "scroll"
  ],
  "license": "MIT",
  "authors": [
    "[object Object]"
  ],
  "ignore": [
    "**/.*",
    "bower_components",
    "tests",
    "test*.html",
    "node_modules",
    "test"
  ],
  "moduleType": [
    "globals"
  ]
}
